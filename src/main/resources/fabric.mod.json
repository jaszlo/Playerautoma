{
	"schemaVersion": 1,
	"id": "playerautoma",
	"version": "${version}",
	"name": "Playerautoma",
	"description": "Record player actions and replay those. Storing of recording is possible in multiple ways",
	"authors": [
		"Jaszlo"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/playerautoma",
		"sources": "https://github.com/jaszlo/Playerautoma/",
		"issues": "https://github.com/jaszlo/Playerautoma/issues"
	},
	"license": "CC0",
	"icon": "assets/playerautoma/icon.png",
	"environment": "client",
	"entrypoints": {
		"client":[
			"net.jasper.mod.PlayerAutomaClient"
		],
		"modmenu":[
			"net.jasper.mod.ModMenuApiImpl"
		]
	},
	"mixins": [
		"playerautoma.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.gg/6XzFPKRFYT",
				"modmenu.modrinth": "https://modrinth.com/mod/playerautoma"
			},
			"update_checker": true
		}
	}
}